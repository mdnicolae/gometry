[
  {
    "identifier": "cli-driver",
    "driver": "cli",
    "default": true
  },
  {
    "identifier": "cli-no-color",
    "driver": "cli",
    "options": {
      "colors": false
    }
  },
  {
    "identifier": "file-driver",
    "driver": "file",
    "options": {
      "file": "app-logs.log"
    }
  },
  {
    "identifier": "prom",
    "driver": "prometheus",
    "options": {
      "job_name": "gometry_job",
      "gateway": "${PROMETHEUS_GATEWAY}"
    }
  }
]